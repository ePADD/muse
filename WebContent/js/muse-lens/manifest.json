{
  "name": "Muse Lens",
  "version": "0.9.11",
  "description": "Muse Lens for Personalized Browsing",
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
